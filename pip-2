 function isAvailable(Doctor){
    if(Doctor.isAvailable != true){
        console.log("Doctor not available at the moment");
    }

    else {
        scheduleAppointment(Doctor);{
            console.log(`${Doctor.name} is assigned to ${Patient.name} at ${Doctor.time}`);

        }
}
 }
console.log(isAvailable("John", "23/4/2024", "Cardiologist", "17.00", "true"));








